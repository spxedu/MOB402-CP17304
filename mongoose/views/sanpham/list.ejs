<h1>Danh sách SP</h1>

<table class="table table-bordered table-striped">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Price</th>
        <th>Desc</th>
    </tr>
    <%  listSP.forEach( (row)  => { %>
        <tr>
            <td> <%= row._id %> </td>
            <td> <%= row.name %> </td>
            <td> <%= row.price %> </td>
            <td> <%= row.description %> </td>
        </tr>
    <%  }); %>
</table>


